{
    "name": "bolechen\/zto-php-sdk",
    "description": "中通开放平台 sdk for php，非官方支持",
    "license": "MIT",
    "authors": [
        {
            "name": "Bole Chen",
            "email": "avenger@php.net"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-json": "*",
        "hanson/foundation-sdk": "1.*|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Bolechen\\Zto\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.*|^7.0"
    },
    "repositories": {
        "omni/contracts": {
            "type": "vcs",
            "url": "git@github.com:CodeNauhc/foundation-sdk.git"
        }
    }
}